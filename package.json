{
  "name": "mcslib",
  "version": "0.1.0",
  "description": "Minecraft command script library",
  "type": "module",
  "main": "build/mcslib.js",
  "exports": {
    ".": "./build/mcslib.js",
    "./*.js": "./build/*.js"
  },
  "scripts": {
    "start": "run-script-os",
    "start:win32": "tsx examples/%npm_config_example%",
    "start:default": "tsx examples/${npm_config_example}",
    "build": "tsc -p tsconfig-build.json",
    "check": "tsc --noEmit --skipLibCheck",
    "lint": "eslint",
    "fix": "eslint --fix"
  },
  "author": "storycraft <storycraft@pancake.sh>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storycraft/mcslib.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@swc/core": "^1.9.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.3",
    "eslint": "9.14.0",
    "run-script-os": "^1.1.6",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a"
}
